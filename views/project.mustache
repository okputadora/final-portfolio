<!DOCTYPE html>
<html dir="ltr" lang="en-US">

{{>head}}

<body class="stretched">

	<!-- Document Wrapper
	============================================= -->
	<div id="wrapper" class="clearfix">

    {{>nav}}

<section id="content" style="background: #f9f9f9">

  <div class="content-wrap">

    <div class="container clearfix">

      <!-- Portfolio Single Gallery
      ============================================= -->
      <div class="col_full text-center">
        <div class="slide"><a href="{{liveLink}}"><img class='img-border' height="400px" src="/imgs/projects/{{image}}" alt={{image}}></a></div>
      </div><!-- .portfolio-single-image end -->

      <div class="col_one_third nobottommargin">

        <!-- Portfolio Single - Meta
        ============================================= -->
        <div class="card events-meta">
          <div class="card-body">
            <ul class="portfolio-meta nobottommargin">
              <li><span><i class="icon-user"></i>Created by:</span> Michael McVeigh</li>
              <li><span><i class="icon-calendar3"></i>Completed on:</span> 17th March 2018</li>
              <li><span><i class="icon-lightbulb"></i>Skills: </span>{{tools}}</li>
            </ul>
          </div>
        </div>
        <!-- Portfolio Single - Meta End -->

      </div>

      <!-- Portfolio Single Content
      ============================================= -->
      <div class="col_two_third portfolio-single-content col_last nobottommargin custom-content">

        <!-- Portfolio Single - Description
        ============================================= -->
        <div class="fancy-title title-dotted-border">
          <h2>Project Info:</h2>
        </div>
        <div class="nobottommargin">
          <p>{{description}}</p>

          <a href={{codeLink}}><i style=" font-size: 30px" class="icon-github2"></i></a></br>
          <a href={{liveLink}}>working example</a>
        </div>
        <!-- Portfolio Single - Description End -->
      </div><!-- .portfolio-single-content end -->
      <div class="clear"></div>
      <div class="divider divider-center"><i class="icon-circle"></i></div>
    </div>
  </div>

</section><!-- #content end -->
{{>footer}}

</div><!-- #wrapper end -->


<!-- External JavaScripts
============================================= -->
<script type="text/javascript"src="/build/vendor.min.js"></script>

</body>
</html>
